:string\r\n    hotel?:IHOTEL | undefined,\r\n    hotels?:IHOTELS[] | undefined\r\n}\r\n\r\ninterface INITIALSTATE {\r\n     Hotels:IHOTELS[]\r\n}\r\n\r\n\r\nconst initialState:INITIALSTATE = {\r\n\r\n  Hotels:[],\r\n\r\n}\r\n\r\n\r\nconst hotelReducer:any = (state=initialState,action:IACTION)=>{\r\n\r\n     switch(action.type){\r\n\r\n       case GET_HOTELS : return {\r\n        \r\n        ...state,\r\n        Hotels:action.hotels\r\n        \r\n\r\n       } \r\n\r\n        case ADD_HOTEL : return {\r\n            ...state,\r\n            Hotels:[...state.Hotels,action.hotel]\r\n\r\n        }\r\n\r\n        \r\n        \r\n\r\n     }\r\n\r\n}\r\n\r\nexport default hotelReducer\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n","C:\\Users\\nader\\OneDrive\\Desktop\\HotelFinder\\client\\src\\redux\\hotel\\hotelTypes.ts",[],"C:\\Users\\nader\\OneDrive\\Desktop\\HotelFinder\\client\\src\\components\\HomePage\\HotelsList.tsx",["178","179","180","181"],"C:\\Users\\nader\\OneDrive\\Desktop\\HotelFinder\\client\\src\\api\\HotelFinder.ts",[],"C:\\Users\\nader\\OneDrive\\Desktop\\HotelFinder\\client\\src\\redux\\hotel\\hotelCreators.ts",[],"C:\\Users\\nader\\OneDrive\\Desktop\\HotelFinder\\client\\src\\utils\\functionsHelper.ts",[],"C:\\Users\\nader\\OneDrive\\Desktop\\HotelFinder\\client\\src\\components\\UpdatePage\\UpdateHotel.tsx",["182","183"],"import React, { useEffect, useState } from 'react'\r\nimport { useHistory, useParams } from 'react-router-dom';\r\nimport { resolveModuleName } from 'typescript';\r\nimport HotelFinder from '../../api/HotelFinder';\r\nimport { IUSEPARAMS } from '../../interfaces/APPINTERFACES';\r\nimport { objBuilder } from '../../utils/functionsHelper';\r\n\r\n\r\n\r\nexport default function UpdateHotel() {\r\n\r\n    const [name,setName] = useState<any>('');\r\n    const [location,setLocation] = useState('');\r\n    const [price_range,setPriceRange] = useState('');\r\n    const [data,setData] = useState<any>('');\r\n\r\n    const {id} = useParams<IUSEPARAMS>();\r\n\r\n    let history = useHistory();\r\n\r\n    useEffect(()=>{\r\n        \r\n        const fetchHotelData = async () =>{\r\n            const response = await HotelFinder.get(\"/\");\r\n\r\n             setData(response.data.data)\r\n           \r\n             setName(data && data.hotel.name)\r\n             setLocation(data && data.hotel.location)\r\n             setPriceRange(data && data.hotel.price_range)\r\n        }\r\n       \r\n       fetchHotelData(); \r\n\r\n    },[])\r\n\r\n    \r\n\r\n  const handleSubmit = async (e:React.MouseEvent<HTMLButtonElement>)=>{\r\n\r\n    e.preventDefault();\r\n\r\n    await HotelFinder.put(`/${id}`,objBuilder(name,location,price_range))\r\n   \r\n    history.push(\"/\");\r\n\r\n  }\r\n\r\n \r\n    \r\n    return (\r\n        \r\n        <div>\r\n            \r\n        <form action=\"\">\r\n           <div className=\"form-group\">\r\n\r\n             <label htmlFor=\"name\">Name</label>\r\n             <input type=\"text\" id=\"name\" className=\"form-control\" value={name} onChange = {e=>setName(e.target.value)} required />\r\n\r\n           </div>\r\n           <div className=\"form-group\">\r\n\r\n             <label htmlFor=\"location\">Location</label>\r\n             <input type=\"text\" id=\"location\" className=\"form-control\" value={location} onChange = {e=>setLocation(e.target.value)} required />\r\n\r\n           </div>\r\n           <div className=\"form-group\">\r\n\r\n             <label htmlFor=\"priceRange\">PriceRange</label>\r\n             <input type=\"number\" id=\"priceRange\" className=\"form-control\"  min=\"1\" max=\"5\" value={price_range} onChange = {e=>setPriceRange(e.target.value)} required />\r\n\r\n           </div>\r\n\r\n           <button onClick={handleSubmit} className=\"btn btn-primary\" type=\"submit\">\r\n               Submit\r\n           </button>\r\n\r\n           <button className=\"btn btn-danger mx-3\" onClick = {()=>history.push(\"/\